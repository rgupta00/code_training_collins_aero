enum ShirtSize{
	S,M,L,XL,XXL;
}
class Shirt{
	private String name;
	private String color;
	private ShirtSize size;
	public Shirt(String name, String color, ShirtSize size) {
		super();
		this.name = name;
		this.color = color;
		this.size = size;
	}
	
	
	@Override
	public String toString() {
		return "Shirt [name=" + name + ", color=" + color + ", size=" + size
				+ "]";
	}

}
public class E_EnumBasics {
