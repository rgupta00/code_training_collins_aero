public class D_GenericMethod {
	
	public static void main(String[] args) {
		
		Integer a=3;
		Integer b=2;
		Integer c=41;
		
		Integer max=maxData(a, b , c);
		System.out.println(max);
		
	}

	private static Integer maxData(Integer a, Integer b, Integer c) {
		Integer max=a;
		if(b>max)
			max=b;
		if(c>max)
			max=c;
		return max;
	}

}




