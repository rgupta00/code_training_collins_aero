package day_1;

public class C_DemoArray {
	
	public static void main(String[] args) {
		int arr[]={2,4,5,7,8,0,11};
		
		//newer syntex: enhance for loop java 5
		for(int temp: arr){
			System.out.println(temp);
		}
		
		
		//more newer syn: stream progamining in java 8
		
		//older syntex
//		for(int i=0;i<arr.length;i++){
//			System.out.println(arr[i]);
//		}
	}

}
