//ann inner class
interface Foodish{
    void food();
}
public class C_AnnInnerClass {
    public static void main(String[] args) {

    	//Syn of annonymous inner class
        Foodish foodish=new Foodish() {
			
			@Override
			public void food() {
				// TODO Auto-generated method stub
				
			}
		};


    }
}
