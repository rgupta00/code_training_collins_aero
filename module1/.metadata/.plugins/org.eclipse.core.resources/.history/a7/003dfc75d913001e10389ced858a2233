		List<Integer> list=new LinkedList<>();
		list.add(12);
		list.add(102);
		list.add(122);
		
		list.add(912);
		
		printList(list);
		
	}

	private static void printList(List<Integer> list) {
		for(Integer val: list){
			System.out.println(val);
		}
	}

}






