package com.day7.session2.mcq.q5;

public class Snippet {
	public static void main(String[] args) {
		//java use a concept of "caching"
		//-128 - to 0 +127
		
		Integer i1 = Integer.valueOf(127);
		Integer i2 = 127;
		
		System.out.println(i1 == i2);//T
		Integer i3 = 128;
		Integer i4 = 128;
		System.out.println(i3 == i4);//F
	}
}
